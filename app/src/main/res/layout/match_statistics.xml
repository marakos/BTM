                android:singleLine="true"
                tools:targetApi="o"></EditText>

            <EditText
                android:id="@+id/twopts_made"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:autofillHints=""
                android:hint="@string/_2pts_made"
                android:inputType=""
                android:singleLine="true"
                tools:targetApi="o"></EditText>

            <EditText
                android:id="@+id/ft_per"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:autofillHints=""
                android:hint="@string/ft_per"
                android:inputType=""
                android:singleLine="true"
                tools:targetApi="o"></EditText>

            <EditText
                android:id="@+id/twopts_per"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:autofillHints=""
                android:hint="@string/_2pts_per"
                android:inputType=""
                android:singleLine="true"
                tools:targetApi="o"></EditText>
        </TableRow>

        <TableRow
