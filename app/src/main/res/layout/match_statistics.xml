            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            tools:layout_editor_absoluteX="43dp"
            tools:layout_editor_absoluteY="92dp">

            <EditText
                android:id="@+id/ft_attempted"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:autofillHints=""
                android:hint="@string/ft_attempted"
                android:inputType=""
                android:singleLine="true"
                tools:layout_conversion_absoluteHeight="45dp"
                tools:layout_conversion_absoluteWidth="111dp"
                tools:layout_editor_absoluteX="26dp"
                tools:layout_editor_absoluteY="137dp"
                tools:targetApi="o"></EditText>

            <EditText
                android:id="@+id/ft_made"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:autofillHints=""
                android:hint="@string/ft_made"
                android:inputType=""
                android:singleLine="true"
                tools:layout_conversion_absoluteHeight="45dp"
                tools:layout_conversion_absoluteWidth="96dp"
                tools:layout_editor_absoluteX="137dp"
                tools:layout_editor_absoluteY="137dp"
                tools:targetApi="o"></EditText>

            <EditText
                android:id="@+id/threepts_att"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:autofillHints=""
                android:hint="@string/_3pts_att"
                android:inputType=""
                android:singleLine="true"
                tools:layout_conversion_absoluteHeight="45dp"
                tools:layout_conversion_absoluteWidth="73dp"
                tools:layout_editor_absoluteX="232dp"
                tools:layout_editor_absoluteY="137dp"
                tools:targetApi="o"></EditText>

            <EditText
                android:id="@+id/twopts_att"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:autofillHints=""
                android:hint="@string/_2pts_att"
                android:inputType=""
                android:singleLine="true"
                tools:layout_conversion_absoluteHeight="45dp"
                tools:layout_conversion_absoluteWidth="88dp"
                tools:layout_editor_absoluteX="306dp"
                tools:layout_editor_absoluteY="137dp"
                tools:targetApi="o"></EditText>

        </TableRow>

        <TableRow
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            tools:layout_editor_absoluteX="43dp"
            tools:layout_editor_absoluteY="92dp">

            <EditText
                android:id="@+id/threepts_made"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:autofillHints=""
                android:hint="@string/_3pts_made"
